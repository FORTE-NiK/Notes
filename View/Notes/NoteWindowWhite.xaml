<Window x:Class="ForteNotes.Notes.NoteWindowWhite"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:ForteNotes.Notes"
		mc:Ignorable="d"
		Title="NoteWindow" 
        Icon="Icon/noteIcon.ico"
        ShowInTaskbar="False"
		Height="{Binding Path=Height, Mode=TwoWay}" Width="{Binding Path=Width, Mode=TwoWay}"
        MinHeight="465" MinWidth="206"
		WindowStyle="None"
        Visibility="{Binding Path=WinVisibility}"
		AllowsTransparency="True"
		ShowActivated="False"
		Background="Transparent"
        Top="{Binding Path= WindowY, Mode=TwoWay}"
        Left="{Binding Path= WindowX, Mode=TwoWay}"
		>
    <Window.Resources>
        <Style x:Key="Resize">
            <Setter Property="WindowChrome.WindowChrome">
                <Setter.Value>
                    <WindowChrome CaptionHeight="0" ResizeBorderThickness="3"/>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="Bt" TargetType="Button">
            <Setter Property="BorderThickness" Value="0" />
            <Setter Property="BorderBrush" Value="Transparent" />
            <Setter Property="Background" Value="Transparent" />

            <Style.Triggers>
                <EventTrigger RoutedEvent="Button.MouseEnter">
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation Duration="0:0:1" Storyboard.TargetProperty="Opacity"  From="1" SpeedRatio="9" To="0.5" />
                        </Storyboard>
                    </BeginStoryboard>
                </EventTrigger>
                <EventTrigger RoutedEvent="Button.MouseLeave">
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation Duration="0:0:1" Storyboard.TargetProperty="Opacity"  From="0.5" SpeedRatio="9" To="1" />
                        </Storyboard>
                    </BeginStoryboard>
                </EventTrigger>
            </Style.Triggers>
        </Style>
        <Style x:Key="ColorBt" TargetType="Button">
            <Setter Property="BorderThickness" Value="0" />
            <Setter Property="BorderBrush" Value="Transparent" />
            <Setter Property="Background" Value="Transparent" />
            <Setter Property="Width" Value="28"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <ContentPresenter Content="{TemplateBinding Content}" />
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <EventTrigger RoutedEvent="Button.MouseEnter">
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation Duration="0:0:1" Storyboard.TargetProperty="Opacity"  From="1" SpeedRatio="9" To="0.5" />
                        </Storyboard>
                    </BeginStoryboard>
                </EventTrigger>
                <EventTrigger RoutedEvent="Button.MouseLeave">
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation Duration="0:0:1" Storyboard.TargetProperty="Opacity"  From="0.5" SpeedRatio="9" To="1" />
                        </Storyboard>
                    </BeginStoryboard>
                </EventTrigger>
            </Style.Triggers>
        </Style>
    </Window.Resources>
    <Border Name="GridNote" Margin="3,0,3,3" BorderThickness="0.8" BorderBrush="{Binding Path=BorderColor}">
        <Border.Effect>
            <DropShadowEffect Color="Black" Direction="-75"
                              BlurRadius="4" ShadowDepth="2" Opacity="0.4"/>
        </Border.Effect>
        <Grid  Background="{Binding Path=WhiteThemeDockColor}" Name="MainGrid">
            <Grid.RowDefinitions >
                <RowDefinition Height="32"/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Border/>
			<Border Grid.Row="0" 
					Panel.ZIndex="2"
					Name="Dock" 
					Margin="0, -25 , 0, 0">
                <DockPanel LastChildFill="False"
						   Background="{Binding Path=WhiteThemeDockColor}" 
						   MouseDown="MoveNote" 
						   Height="32" 
						   Name="DockColor"
						   VerticalAlignment="Bottom">
                    <Button Width="32" 
							Style="{StaticResource Bt}" 
							Name="CloseNoteButton"
							Height="32" 
							DockPanel.Dock="Right" 
							Command="{Binding Path=CloseNoteButton}">
						<Image Source="ButtonsRes/DockButtons/Close.png" Width="20" Height="20"/>
						<Button.Triggers>
							<EventTrigger RoutedEvent="Button.Click">
								<BeginStoryboard>
									<Storyboard>
										<DoubleAnimation Duration="0:0:1" Storyboard.TargetProperty="Opacity"  Storyboard.TargetName="GridNote" From="1" SpeedRatio="7" To="0" />
									</Storyboard>
								</BeginStoryboard>
							</EventTrigger>
						</Button.Triggers>
					</Button>
					<Button Height="32"
							Style="{StaticResource Bt}"
							Width="32" 
							DockPanel.Dock="Right"
							>
						<Image Source="ButtonsRes/DockButtons/Menu.png" Width="16" Height="16" />
						<Button.Triggers>
							<EventTrigger RoutedEvent="Button.Click">
								<BeginStoryboard>
									<Storyboard>
										<DoubleAnimation Storyboard.TargetName="ColorPickMenu" Duration="0:0:1" SpeedRatio="15" Storyboard.TargetProperty="Height" To="110">
                                            <DoubleAnimation.EasingFunction>
                                                <ElasticEase EasingMode="EaseInOut" Oscillations="5" Springiness="4" />
                                            </DoubleAnimation.EasingFunction>
                                        </DoubleAnimation>
									</Storyboard>
								</BeginStoryboard>
							</EventTrigger>
						</Button.Triggers>
					</Button>
					<Button Height="32"
							Command="{Binding Path=CreateNote}"
							Style="{StaticResource Bt}"
							Width="32" 
							DockPanel.Dock="Left">
						<!--Click="CreateNote"-->
						<Image Source="ButtonsRes/DockButtons/Create.png" 
							   Width="20"
							   Height="20"/>
					</Button>
				</DockPanel>
