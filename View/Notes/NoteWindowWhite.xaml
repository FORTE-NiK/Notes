                        <TextBox Grid.Column="1" Name="AdditionalNameBox" Background="{Binding Path=WhiteThemeDockColor}" Foreground="{Binding Path=TextColorWhite}" BorderThickness="0" Text="{Binding Path=AdditionalHeaderText}" TextAlignment="Center" FontWeight="Bold"/>
                        <TextBox Grid.Column="2" Name="AdditionalBox" Foreground="{Binding Path=TextColorWhite}" Background="{Binding Path=WhiteThemeDockColor}" BorderThickness="0" Text="{Binding Path=AdditionalDate}" TextAlignment="Right" FontWeight="Bold"/>
                    </Grid>
                    <TextBox Grid.Row="1" BorderBrush="#FFF7D1" Panel.ZIndex="0"
                             BorderThickness="0"
                             FontSize="14"
                             Text="{Binding Path=AdditionalText}"
                             Name="AdditionalNoteText"
                             TextWrapping="Wrap"
                             AcceptsReturn="True"
                             Padding="2,1,2,1"
                             Background="{Binding Path=WhiteThemeTextBoxColor}"
                             Foreground="{Binding Path=TextColorWhite}" />
                </Grid>
                <Grid.Triggers>
                    <EventTrigger RoutedEvent="Mouse.PreviewMouseDown">
                        <BeginStoryboard>
                            <Storyboard>
                                <DoubleAnimation Storyboard.TargetName="ColorPickMenu" Duration="0:0:0:5"
                                                 SpeedRatio="35" Storyboard.TargetProperty="Height" To="0">
                                    <DoubleAnimation.EasingFunction>
                                        <CubicEase EasingMode="EaseIn" />
                                    </DoubleAnimation.EasingFunction>
                                </DoubleAnimation>
                            </Storyboard>
                        </BeginStoryboard>
                    </EventTrigger>
                </Grid.Triggers>
            </Grid>
        </Grid>
    </Border>
    <Window.Triggers>
        
        <EventTrigger RoutedEvent="Window.Loaded">
            <BeginStoryboard>
                <Storyboard>
                    <DoubleAnimation Duration="0:0:1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="GridNote" From="0" SpeedRatio="7" To="1" />
                </Storyboard>
            </BeginStoryboard>
            <BeginStoryboard>
                <Storyboard>
                    <ThicknessAnimation BeginTime="0:0:1" Duration="0:0:0:3" SpeedRatio="30" To="0,0,0,24" Storyboard.TargetName="Dock" Storyboard.TargetProperty="Margin"/>
                    <DoubleAnimation Storyboard.TargetName="ColorPickMenu" Duration="0:0:0:5" SpeedRatio="35" Storyboard.TargetProperty="Height" To="0">
                        <DoubleAnimation.EasingFunction>
                            <CubicEase EasingMode="EaseIn"/>
                        </DoubleAnimation.EasingFunction>
                    </DoubleAnimation>
                </Storyboard>
            </BeginStoryboard>
        </EventTrigger>
        <EventTrigger RoutedEvent="Mouse.MouseLeave">
            <BeginStoryboard>
                <Storyboard>
                    <ThicknessAnimation BeginTime="0:0:1" Duration="0:0:0:3" SpeedRatio="30" To="0,0,0,24" Storyboard.TargetName="Dock" Storyboard.TargetProperty="Margin"/>
                    <DoubleAnimation Storyboard.TargetName="ColorPickMenu" Duration="0:0:0:5" SpeedRatio="35" Storyboard.TargetProperty="Height" To="0">
                        <DoubleAnimation.EasingFunction>
                            <CubicEase EasingMode="EaseIn"/>
                        </DoubleAnimation.EasingFunction>
                    </DoubleAnimation>
                </Storyboard>
            </BeginStoryboard>
        </EventTrigger>
        <EventTrigger RoutedEvent="Mouse.PreviewMouseDown">
            <BeginStoryboard>
                <Storyboard>
                    <ThicknessAnimation Duration="0:0:0:3" SpeedRatio="30" To="0,0,0,0" Storyboard.TargetName="Dock" Storyboard.TargetProperty="Margin"/>
                </Storyboard>
            </BeginStoryboard>
        </EventTrigger>
    </Window.Triggers>
</Window>
